{{> menu id="dashboard"}}

{{#each playlists}}
  <section class="ui segment">
    <h2 class="ui header">
      {{title}}
    </h2>
    <p> Total Duration: {{duration}} </p>
    <a href="/playlist/{{id}}"><i class="folder icon"></i></a>
    <a href="/playlist/{{id}}/deletePlaylist/{{id}}" class="ui tiny red button delPlaylist">Delete Playlist</a>
      
  </section>
{{/each}}
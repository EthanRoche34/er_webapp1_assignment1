{{> menu id="dashboard"}}

<section class="ui segment">
  {{#each playlists}}
      <h2 class="ui header">
        {{title}}
      </h2>
  <h3 class="ui header">
    {{duration}}
  </h3>
      {{> listsongs}}
  {{/each}}

  <p>
    <span id="rating"> </span>
  </p>

  <button class="ui blue button" onclick="getRating()">
    Rate it! <i class="star icon"></i>
  </button>
</section>

<table class="ui fixed striped table">
  <thead>
    <tr>
      <th>Song</th>
      <th>Artist</th>
      <th>Song Duration</th>
      <th>Genre</th>
    </tr>
  </thead>
  <tbody>
    {{#each songs}}
      <tr>
        <td>
          {{title}}
        </td>
        <td>
          {{artist}}
        </td>
        <td>
          {{songDuration}}
        </td>
        <td>
          {{genre}}
        </td>
      </tr>
    {{/each}}
  </tbody>
</table>